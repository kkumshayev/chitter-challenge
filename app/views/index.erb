<%if current_user%>
  Welcome, <%=current_user.username%>
<%end%>
<br>
<br>

<form action='/users/new' align='center'>
  <input type='submit' value='Sign Up'>
</form>
<br>
<form action='/peep' method='post' align='center'>
  <input type='text' name="peep">
  <input type='submit' value='Post'>
</form>
<br>
<center>
<div id='peeps'>
  <% @peeps.reverse_each do |peep| %>
    <li>
      <%= "\"#{peep.content}\"" %>
      <!-- <%= Time.now.strftime("Peeped on %d/%m/%y at %I:%M%p") %> -->
    </li>
  <% end %>
</div>
</center>
